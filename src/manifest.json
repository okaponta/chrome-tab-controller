{
    "name": "Chrome Tab Controller",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Support Moving Tabs",
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "move-to-first": {
          "suggested_key": "Ctrl+Shift+1",
          "description": "moves the tab to the left"
        }
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {}
}